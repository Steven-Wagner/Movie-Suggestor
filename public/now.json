{"version": 2,
    "alias": "stevenwagner-movie-suggester",
    "name": "movie-suggester",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "build": {
      "env": {
        "REACT_APP_OMDB_API_KEY": "@omdb_api_key",
        "REACT_APP_BASE_URL": "@react_app_base_url"
      } 
    } 
}
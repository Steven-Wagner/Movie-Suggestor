{"version": 2,
    "alias": "stevenwagner-movie-suggester",
    "name": "movie-suggester",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "build": {
      "env": {
        "REACT_APP_BASE_URL": "https://arcane-caverns-82267.herokuapp.com/api"
      }
    },
    "env": {
        "REACT_APP_OMDB_API_KEY": "@omdb_api_key"
      }  
}